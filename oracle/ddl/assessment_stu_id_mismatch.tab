create table EDFIDATA.ASSESSMENT_STU_ID_MISMATCH
(
  last_name       VARCHAR2(50),
  first_name      VARCHAR2(30),
  birth_date      VARCHAR2(10),
  loaded_date     DATE not null,
  district_id     VARCHAR2(6) not null,
  file_name       VARCHAR2(100) not null,
  assessment_type VARCHAR2(6) not null,
  student_id      VARCHAR2(10)
)
;
create index EDFIDATA.IX1_STU_ID_MISMATCH on EDFIDATA.ASSESSMENT_STU_ID_MISMATCH (FILE_NAME);
create index EDFIDATA.IX2_STU_ID_MISMATCH on EDFIDATA.ASSESSMENT_STU_ID_MISMATCH (DISTRICT_ID, FILE_NAME, ASSESSMENT_TYPE);
create index EDFIDATA.IX_ASSESSMENT_STU_ID_MISMATCH on EDFIDATA.ASSESSMENT_STU_ID_MISMATCH (DISTRICT_ID, LOADED_DATE, FILE_NAME);

